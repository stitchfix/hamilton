    "# Cell 1 - import the things you need\n",
    "import logging\n",
    "import sys\n",
    "\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "from hamilton import ad_hoc_utils, driver\n",
    "\n",
    "logging.basicConfig(stream=sys.stdout)"
   ]
  },
